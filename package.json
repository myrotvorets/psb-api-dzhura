{
  "name": "psbapi-microservice-dzhura",
  "version": "1.7.1",
  "private": true,
  "description": "Dzhura integration microservice",
  "main": "dist/src/index.js",
  "scripts": {
    "wait-for-mysql": "wait-on \"tcp:$MYSQL_HOST:3306\" -t 180000",
    "build": "tsc",
    "start:dev": "nodemon -L",
    "lint": "eslint -f gha .",
    "lint:fix": "eslint --fix .",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test jest",
    "posttest": "npm run build",
    "prescan": "npm run wait-for-mysql && ts-node ./test/migrate.ts",
    "scan": "npm run start:dev"
  },
  "keywords": [],
  "author": "Myrotvorets <support@myrotvorets.center> (https://myrotvorets.center/)",
  "license": "MIT",
  "dependencies": {
    "@cloudnative/health-connect": "^2.1.0",
    "@myrotvorets/create-server": "^1.1.0",
    "@myrotvorets/express-async-middleware-wrapper": "^2.0.3",
    "@myrotvorets/express-microservice-middlewares": "^1.4.2",
    "@myrotvorets/oav-installer": "^2.2.1",
    "@myrotvorets/opentelemetry-configurator": "^2.0.1",
    "@myrotvorets/opentelemetry-plugin-knex": "^0.24.0",
    "@opentelemetry/instrumentation-express": "^0.31.0",
    "envalid": "^7.0.0",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.10.11",
    "knex": "^2.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "objection": "^3.0.0-alpha.4",
    "wpautop": "^1.0.0"
  },
  "devDependencies": {
    "@myrotvorets/eslint-config-myrotvorets-ts": "^2.2.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/mock-knex": "^0.4.3",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^17.0.0",
    "@types/supertest": "^2.0.10",
    "eslint-formatter-gha": "^1.2.0",
    "eslint-plugin-jest": "^27.0.0",
    "jest": "^29.0.0",
    "jest-github-actions-reporter": "^1.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "mock-knex": "^0.4.9",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.5.4",
    "wait-on": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myrotvorets/psb-api-microservice-dzhura.git"
  },
  "bugs": {
    "url": "https://github.com/myrotvorets/psb-api-microservice-dzhura/issues"
  },
  "homepage": "https://github.com/myrotvorets/psb-api-microservice-dzhura#readme"
}
