{
  "name": "psbapi-microservice-dzhura",
  "version": "2.0.1",
  "private": true,
  "description": "Dzhura integration microservice",
  "type": "module",
  "scripts": {
    "wait-for-mysql": "wait-on \"tcp:$KNEX_HOST:3306\" -t 180000",
    "build": "tsc",
    "postbuild": "copyfiles -e '**/*.mts' -e '**/*.ts' 'src/**/*' 'test/**/*' dist/",
    "start:dev": "nodemon",
    "lint": "eslint -f gha .",
    "lint:fix": "eslint --fix .",
    "pretest": "npm run lint",
    "test": "mocha",
    "posttest": "npm run build",
    "pretest:coverage": "rimraf dist; npm run build",
    "test:coverage": "cross-env JS_TESTS=1 c8 mocha dist/test",
    "prescan": "npm run build; npm run wait-for-mysql; cross-env SEED_TABLES=yes node ./dist/test/migrate.mjs",
    "scan": "node ./dist/src/index.mjs"
  },
  "keywords": [],
  "author": "Myrotvorets <support@myrotvorets.center> (https://myrotvorets.center/)",
  "license": "MIT",
  "dependencies": {
    "@cloudnative/health-connect": "^2.1.0",
    "@myrotvorets/express-async-middleware-wrapper": "^2.2.0",
    "@myrotvorets/express-microservice-middlewares": "^2.2.0",
    "@myrotvorets/express-request-logger": "^1.2.0",
    "@myrotvorets/oav-installer": "^5.0.0",
    "@myrotvorets/opentelemetry-configurator": "^7.1.0",
    "@myrotvorets/opentelemetry-plugin-knex": "^0.30.0",
    "@myrotvorets/otel-utils": "^1.0.1",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/core": "^1.17.1",
    "@opentelemetry/semantic-conventions": "^1.17.1",
    "awilix": "^9.0.0",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.0.6",
    "knex": "^2.5.1",
    "mysql2": "^3.6.2",
    "wpautop": "^1.0.0"
  },
  "devDependencies": {
    "@myrotvorets/eslint-config-myrotvorets-ts": "^2.22.6",
    "@types/chai": "^4.3.9",
    "@types/chai-as-promised": "^7.1.7",
    "@types/express": "^4.17.20",
    "@types/mocha": "^10.0.3",
    "@types/mock-knex": "^0.4.7",
    "@types/node": "^20.8.10",
    "@types/supertest": "^2.0.15",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint-formatter-gha": "^1.4.3",
    "eslint-plugin-mocha": "^10.2.0",
    "mocha": "^10.2.0",
    "mocha-multi": "^1.1.7",
    "mocha-reporter-gha": "^1.1.1",
    "mocha-reporter-sonarqube": "^2.0.5",
    "mock-knex": "^0.4.12",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "wait-on": "^7.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myrotvorets/psb-api-dzhura.git"
  },
  "bugs": {
    "url": "https://github.com/myrotvorets/psb-api-dzhura/issues"
  },
  "homepage": "https://github.com/myrotvorets/psb-api-dzhura#readme"
}
